<?xml version="1.0" encoding="UTF-8"?>
<bundle>
  <!--
  ###################################################################
  #      org.exoplatform.ecm.webui.component.browse.UIConfigTabPane #
  ###################################################################
  -->
  <UIConfigTabPane>
    <tab>
      <label>
        <UIPathConfig>Path Config</UIPathConfig>
        <UINewConfigForm>Create New</UINewConfigForm>
      </label>
    </tab>
  </UIConfigTabPane>
  <!--
  ###################################################################
  #      org.exoplatform.ecm.webui.component.browse.UIPathConfig #
  ###################################################################
  -->
  <!--
    ###################################################################
		#	    FROM VERSION DMS 2.3-SNAPSHOT, we add filterCategory        #
		#	    to display checkbox for filtering category tree             #
		#	    base on permission of user                                  #
    ###################################################################
   -->
  <UIPathConfig>
    <label>
      <categoryPath>Categories Path</categoryPath>
      <workspace>Workspace</workspace>
      <repository>Repository</repository>
      <path>UIPathConfig</path>
      <template>Template</template>
      <itemPerPage>Items Per Page</itemPerPage>
      <detailBoxTemp>Detail Box Template</detailBoxTemp>
      <enableToolBar>View Toolbar</enableToolBar>
      <enableRefDoc>Reference Document</enableRefDoc>
      <filterCategory>Filters out categories based on current user permissions</filterCategory>
      <enableChildDoc>Children Document</enableChildDoc>
      <enableTagMap>Tagmap</enableTagMap>
      <enableComment>Commentable</enableComment>
      <enableVote>Voteable</enableVote>
      <isAllowPublish>Show only documents in the published state</isAllowPublish>
    </label>
    <title>Path Config</title>
    <action>
      <Add>New config</Add>
      <Save>#{word.save}</Save>
      <Edit>Edit</Edit>
      <Cancel>#{word.cancel}</Cancel>
      <Back>#{word.back}</Back>
    </action>
    <msg>
      <require-path>You must select the category path.</require-path>
      <invalid-path>The category path is invalid.</invalid-path>
      <invalid-value>The page number is invalid.</invalid-value>
      <invalid-number>You must input a number.</invalid-number>
      <access-denied>Access denied. You don't have permission in this category path.</access-denied>
    </msg>
  </UIPathConfig>
  <UIQueryConfig>
    <msg>
      <repostoryException>Maybe, you have problem with your query. Please check again.</repostoryException>
      <noSuchNodeTypeException>Maybe, you have problem with your query. Please check again.</noSuchNodeTypeException>
      <invalid-value>The page number is invalid.</invalid-value>
      <invalid-query>The query string is invalid.</invalid-query>
      <invalid-name>The query name not found. Please try other type, language and status.</invalid-name>
    </msg>
    <title>Query Config</title>
    <label>
      <repository>Repository</repository>
      <workspace>Workspace</workspace>
      <queryLanguage>Query Language</queryLanguage>
      <queryStatus>Query Status</queryStatus>
      <queryType>Query Type</queryType>
      <queryStore>Query Name</queryStore>
      <query>Query</query>
      <template>Template</template>
      <itemPerPage>Items per Page</itemPerPage>
      <detailBoxTemp>Detail Box Template</detailBoxTemp>
      <enableTagMap>Tagmap</enableTagMap>
      <enableComment>Commentable</enableComment>
      <enableVote>Voteable</enableVote>
      <isAllowPublish>Show only documents in "published" state</isAllowPublish>
    </label>
    <action>
      <Add>New config</Add>
      <Save>#{word.save}</Save>
      <Edit>Edit</Edit>
      <Cancel>#{word.cancel}</Cancel>
      <Back>#{word.back}</Back>
    </action>
  </UIQueryConfig>
  <!--
  ###################################################################
  #      org.exoplatform.ecm.webui.component.browse.UIQueryConfig #
  ###################################################################
  -->
  <path>
    <title>
      <AddPath>Add Path</AddPath>
    </title>
  </path>
  <rootNodeInfo>
    <title>
      <AddRootNode>Add Root Node</AddRootNode>
    </title>
  </rootNodeInfo>
  <!--
  #############################################################
  # org.exoplatform.ecm.webui.component.UIVoteForm.gtmpl		#
  #############################################################
  -->
  <UIVoteForm>
    <title>
      <normal>Normal/Click to vote</normal>
      <good>Good/Click to vote</good>
      <verygood>Very Good/Click to vote</verygood>
      <excellent>Excellent/Click to vote</excellent>
      <best>Best/Click to vote</best>
    </title>
    <button>
      <label>
        <cancel>Cancel</cancel>
      </label>
    </button>
  </UIVoteForm>
  <!--
  ###################################################################
  #      org.exoplatform.ecm.webui.component.browse.UIScriptConfig #
  ###################################################################
  -->
  <UIScriptConfig>
    <title>Script Config</title>
    <label>
      <repository>Repository</repository>
      <workspace>Workspace</workspace>
      <scriptName>Script Name</scriptName>
      <template>Template</template>
      <detailBoxTemp>Detail Box Template</detailBoxTemp>
      <enableTagMap>Tagmap</enableTagMap>
      <enableComment>Commentable</enableComment>
      <enableVote>Voteable</enableVote>
      <isAllowPublish>Show only documents in the published state</isAllowPublish>
    </label>
    <action>
      <Add>New Config</Add>
      <Save>#{word.save}</Save>
      <Edit>Edit</Edit>
      <Back>#{word.back}</Back>
      <Cancel>#{word.cancel}</Cancel>
    </action>
  </UIScriptConfig>
  <!--
  ###################################################################
  #    org.exoplatform.ecm.webui.component.browse.UIDocumentConfig #
  ###################################################################
  -->
  <UIDocumentConfig>
    <title>Document Config</title>
    <label>
      <repository>Repository</repository>
      <workspace>Workspace</workspace>
      <categoryPath>Categories Path</categoryPath>
      <path>UIDocumentConfig</path>
      <doc>UIDocumentConfig</doc>
      <docName>Document Name</docName>
      <detailBoxTemp>Detail Box Template</detailBoxTemp>
      <enableComment>Commentable</enableComment>
      <enableVote>Voteable</enableVote>
      <isAllowPublish>Show only documents in the published state</isAllowPublish>
    </label>
    <action>
      <Add>New Config</Add>
      <Save>#{word.save}</Save>
      <Edit>Edit</Edit>
      <Back>#{word.back}</Back>
      <Cancel>#{word.cancel}</Cancel>
    </action>
    <msg>
      <require-path>You must select the category path first.</require-path>
      <invalid-path>Invalid category path. Please select the path again.</invalid-path>
      <require-doc>You must select a document.</require-doc>
      <invalid-doc>This document is deleted or changed or you don't have permission to access.</invalid-doc>
    </msg>
  </UIDocumentConfig>
  <!--
  ###################################################################
  #      org.exoplatform.ecm.webui.component.browse.UINewConfigForm #
  ###################################################################
  -->
  <UINewConfigForm>
    <label>
      <repository>Select Repository</repository>
      <workspace>Workspace</workspace>
      <browseType>Browse Content</browseType>
    </label>
    <title>Create New</title>
    <action>
      <Next>Next</Next>
      <Back>#{word.back}</Back>
    </action>
  </UINewConfigForm>
  <!--
  ###################################################################
  #   org.exoplatform.ecm.webui.component.browse.UIToolBar      #
  ###################################################################
  -->
  <UIToolBar>
    <label>
      <RootNode>Root</RootNode>
    </label>
    <tooltip>
      <Hidden>Hidden Tree</Hidden>
      <Show>Show Tree</Show>
      <showSearch>Show/Hide Search Form</showSearch>
      <Back>Back</Back>
      <Vote>Vote</Vote>
      <Comment>Write Comment</Comment>
    </tooltip>
    <msg>
      <back-view-search>Back to the previous view for search.</back-view-search>
      <select-doc>No document was selected or has been removed.</select-doc>
      <node-removed>The node has been removed.</node-removed>
      <readonly-doc>The current document is checked in. Please check out it first.</readonly-doc>
      <not-support-vote>The Vote function is not supported for this document type.</not-support-vote>
      <not-support-comment>The Comment function is not supported for this document type.</not-support-comment>
      <node-is-locked>The node is locked. Please unlock it first.</node-is-locked>
      <access-add-denied>Access denied. You don't have the add permission in this node.</access-add-denied>
    </msg>
  </UIToolBar>
  <!--
  ###################################################################
  #   org.exoplatform.ecm.webui.component.browse.UISearchController #
  ###################################################################
  -->
  <UISearchController>
    <label>
      <nodePath>You are looking in</nodePath>
      <searchTime>Result</searchTime>
      <duration>Duration</duration>
      <nodeName>Node</nodeName>
    </label>
  </UISearchController>
  <!--
  ###################################################################
  #      org.exoplatform.ecm.webui.component.browse.UICBSearchForm #
  ###################################################################
  -->
  <UICBSearchForm>
    <label>
      <inputValue>Search Keywords</inputValue>
      <option>nolabel</option>
      <search>Search now</search>
      <referencesDoc>References</referencesDoc>
      <childDoc>Children</childDoc>
    </label>
    <title>
      <search>Search now</search>
    </title>
    <msg>
      <not-empty>You must enter keyword.</not-empty>
      <suggestion-keyword>Suggestions '{0}' not found. Please try different keywords.</suggestion-keyword>
    </msg>
  </UICBSearchForm>
  <!--
  ###################################################################
  #    org.exoplatform.ecm.webui.component.browse.UICBSearchResults #
  ###################################################################
  -->
  <UICBSearchResults>
    <header>
      <name>Name</name>
      <path>Path</path>
      <action>Action</action>
    </header>
    <action>
      <Close>#{word.close}</Close>
    </action>
    <msg>
      <node-removed>Missing node: it has been moved or deleted.</node-removed>
      <not-support>The View template is not supported for this node.</not-support>
      <not-permission>Access denied. Maybe, you don't have privileges to access this node.</not-permission>
      <empty>No results found.</empty>
    </msg>
    <tooltip>
      <Goto>Go to folder</Goto>
      <View>View</View>
    </tooltip>
  </UICBSearchResults>
  <!--
  ###################################################################
  #    org.exoplatform.ecm.webui.component.browse.UIBrowseContainer #
  ###################################################################
  -->
  <UIBrowseContainer>
    <label>
      <NoDocument>No document in this category</NoDocument>
      <author>Author</author>
      <summary>Summary</summary>
      <more>More Documents</more>
      <previous>Previous View</previous>
      <path-Previous>Previous</path-Previous>
      <path-Root>Root</path-Root>
      <home>Home</home>
      <cannot-access>Access denied. Maybe, you don't have the view permission in this portlet configuration.</cannot-access>
    </label>
    <title>
      <home>Home</home>
      <prevTabs>Previous Tabs</prevTabs>
      <moreTabs>More Tabs</moreTabs>
    </title>
    <msg>
      <template-notsupported>No view template to load.</template-notsupported>
      <invalid-node>Missing node : it has been moved or deleted or you don't have enough permission to access</invalid-node>
    </msg>
  </UIBrowseContainer>
  <!--
  ###################################################################
  #      org.exoplatform.ecm.webui.component.JCRBrowser #
  ###################################################################
  -->
  <pathSelector>
    <title>
      <UIOneNodePathSelector>Path Select</UIOneNodePathSelector>
      <pathSelector>Path Selector</pathSelector>
    </title>
  </pathSelector>
  <documentSelector>
    <title>
      <UIOneNodePathSelector>Document Select</UIOneNodePathSelector>
    </title>
  </documentSelector>
  <UITreeList>
    <header>
      <name>Name</name>
      <action>Action</action>
    </header>
    <info>
      <child-not-found>Have no child</child-not-found>
    </info>
  </UITreeList>
  <!--
  ###################################################################
  #      org.exoplatform.ecm.webui.component.browse.UICBCommentForm #
  ###################################################################
  -->
  <UICBCommentForm>
    <label>
      <title>Add Comment</title>
      <name>Name</name>
      <email>Email</email>
      <website>Website</website>
      <comment>Comment</comment>
    </label>
    <action>
      <Cancel>#{word.cancel}</Cancel>
      <Save>#{word.save}</Save>
    </action>
    <msg>
      <locked-doc>The document is locked.</locked-doc>
      <versioning-doc>The document is versioning.</versioning-doc>
      <error-vote>An error occurred while commenting on the document.</error-vote>
      <comment-required>The comment is required.</comment-required>
    </msg>
  </UICBCommentForm>
  <UICBVoteForm>
    <msg>
      <locked-doc>The document is locked.</locked-doc>
      <versioning-doc>The document is versioning.</versioning-doc>
      <error-vote>An error occurred while voting for the document.</error-vote>
    </msg>
  </UICBVoteForm>
  <!--
  ###################################################################
  #      org.exoplatform.ecm.webui.component.browse.UIPopupWindow #
  ###################################################################
  -->
  <UICBPopupWindow>
    <title>
      <UICBCommentForm>Add Comment</UICBCommentForm>
      <UICBVoteForm>Vote This</UICBVoteForm>
      <UIDocumentDetail>View Document</UIDocumentDetail>
    </title>
  </UICBPopupWindow>
  <!--
  ###################################################################################
  # JCR Browser																			  #
  ###################################################################################
  -->
  <UIWorkspaceList>
    <label>
      <workspaceName>Workspace</workspaceName>
      <rootNodeInfo>Root Path</rootNodeInfo>
      <rootPath>Root Path</rootPath>
    </label>
  </UIWorkspaceList>
  <!--
  ###################################################################################
  # UITagList																		  #
  ###################################################################################
  -->
  <UITagList>
    <label>
      <tagTitle>Tag Map</tagTitle>
    </label>
  </UITagList>
  <UICategoryTree>
    <label>
      <title>Categories</title>
    </label>
    <msg>
      <invalid-node>Missing node : it has been moved or deleted</invalid-node>
    </msg>
  </UICategoryTree>
  <!--
  ###################################################################################
  #  org.exoplatform.ecm.webui.component.browse.UIBrowseContentHelp																  #
  ###################################################################################
  -->
  <UIBrowseContentHelp>
    <label>
      <info>You are in the help mode, you can configure your porlet by selecting the edit mode.</info>
    </label>
  </UIBrowseContentHelp>
  <UIFormInputSetWithAction>
    <msg>
      <confirm-delete>Are you sure to delete?</confirm-delete>
    </msg>
  </UIFormInputSetWithAction>
  <!--
  ###################################################################################
  # org.exoplatform.ecm.jcr.SearchValidator																			#
  ###################################################################################
  -->
  <SearchValidator>
    <msg>
      <empty-input>The value of this field is null or empty.</empty-input>
      <Invalid-char>This field contains some invalid characters. Please enter another value.</Invalid-char>
      <Invalid-char1>Number following the char '~' must between 0 to 1</Invalid-char1>
      <Invalid-char2>Number must follow the char '~'</Invalid-char2>
      <Invalid-char3>Number must follow the char '^'</Invalid-char3>
      <Invalid-char4>No space is allowed near the char '~'</Invalid-char4>
      <Invalid-char5>No space is allowed near the char '^'</Invalid-char5>
    </msg>
  </SearchValidator>
  <!--
  ######################################################################################
  # Since ECM 2.3
  ######################################################################################
  -->
  <UIPathConfig>
    <label>
      <searchEnable>Enable Search Location</searchEnable>
      <searchLocation>Search Location</searchLocation>
      <searchPath>UIPathConfig</searchPath>
      <explain></explain>
    </label>
    <msg>
      <enter-search-location>Please specify the search location.</enter-search-location>
      <explain>When the path shown in Browse Content corresponds to a hierarchy of categories, it is needed to specify the path where documents are actually stored, so that they can be found by the search feature of the Portlet.</explain>
    </msg>
  </UIPathConfig>
  
  <UIDocumentForm>
  	<msg>
  		<not-support>Cannot load the template of '{0}'.</not-support>
  		<do-not-permission>You don't have permission to load the template of '{0}'.</do-not-permission>
  	</msg>
  </UIDocumentForm>
  
   <!--
	  ######################################################################################
	  # Since ECM 2.6
	  ######################################################################################
	  -->
  <Comments>
	  <tooltip>
	    <remove-comment>Remove this comment</remove-comment>
	    <edit-comment>Edit this comment</edit-comment>
	  </tooltip>
	</Comments>
	<UIDocumentDetail>
	  <msg>
	    <confirm-deletecomment>Are you sure to delete this comment?</confirm-deletecomment>
	  </msg>
	</UIDocumentDetail>
	<UIPopupWindow>
			<title>
				<UICBCommentForm>Input Comment</UICBCommentForm>
			</title> 
		</UIPopupWindow>
	<UIBrowseContainer>
    <msg>
			<symlinkNode-no-targetNode>The target node has been moved or deleted, or You don't have permission to access.</symlinkNode-no-targetNode>
    </msg>
  </UIBrowseContainer>		
  <UIDocumentDetail>
  	<tooltip>
  		<remove-attachment>Delete this attached file</remove-attachment>
  	</tooltip>
  </UIDocumentDetail>
  <DocumentDetail>
  	<tooltip>
  		<remove-attachment>Delete this attached file</remove-attachment>
  	</tooltip>
  </DocumentDetail>
  
 <UITagList>
  <label>
    <PublicTags>Public Tags</PublicTags>
    <PrivateTags>Private Tags</PrivateTags>
   </label>
 </UITagList>
  
</bundle>

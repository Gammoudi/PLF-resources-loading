<?xml version="1.0" encoding="UTF-8"?>
<bundle>
  <UINewsletterViewerForm>
    <msg>
      <updateSuccess>Your subscription was updated successfully.</updateSuccess>
      <subcribed>You are now subscribed to the selected newletters.</subcribed>
      <email-not-empty>The Email field is required.</email-not-empty>
      <checkSubscriptionToProcess>Please select at least one subscription before submitting.</checkSubscriptionToProcess>
      <alreadyExistedEmail>This email already exists. Please input another one.</alreadyExistedEmail>
      <canNotSubcribed>Cannot subscribe. Please try again.</canNotSubcribed>
      <get-checked>An error occurred while trying to access the checked subscription.</get-checked>
    </msg>
    <action>
      <Subcribe>Subscribe</Subcribe>
      <ForgetEmail>Forget this email</ForgetEmail>
      <ChangeSubcriptions>Change your subscription</ChangeSubcriptions>
    </action>
    <title>
      <userEmail>Your Email</userEmail>
      <subscription>Subscription</subscription>
      <yourStatus>Check to subscribe</yourStatus>
    </title>
    <Email>
      <ConfirmUser>
        <Subject>Newsletter confirmation request</Subject>
        <Content>Click #here# to confirm your subscription.</Content>
      </ConfirmUser>
    </Email>
  </UINewsletterViewerForm>
</bundle>
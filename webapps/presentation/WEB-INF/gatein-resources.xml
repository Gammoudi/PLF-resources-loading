<gatein-resources xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xsi:schemaLocation="http://www.gatein.org/xml/ns/gatein_resources_1_2_1 http://www.gatein.org/xml/ns/gatein_resources_1_2_1" 
    xmlns="http://www.gatein.org/xml/ns/gatein_resources_1_2_1">

  <!-- SingleContentViewer skins -->

  <portlet-skin>
    <application-name>presentation</application-name>
    <portlet-name>SingleContentViewer</portlet-name>
    <skin-name>Default</skin-name>
    <css-path>/skin/SingleContentViewer/DefaultSkin/Stylesheet.css</css-path>
  </portlet-skin>
  
  <!-- CategoryNavigationPortlet skins -->

  <portlet-skin>
    <application-name>presentation</application-name>
    <portlet-name>CategoryNavigationPortlet</portlet-name>
    <skin-name>Default</skin-name>
    <css-path>/skin/ContentListViewer/DefaultStylesheet.css</css-path>
  </portlet-skin>
  
  <!-- ContentListViewerPortlet skins -->

  <portlet-skin>
    <application-name>presentation</application-name>
    <portlet-name>ContentListViewerPortlet</portlet-name>
    <skin-name>Default</skin-name>
    <css-path>/skin/ContentListViewer/DefaultStylesheet.css</css-path>
  </portlet-skin>
  
  <!-- ParameterizedContentViewerPortlet skins -->

  <portlet-skin>
    <application-name>presentation</application-name>
    <portlet-name>ParameterizedContentViewerPortlet</portlet-name>
    <skin-name>Default</skin-name>
    <css-path>/skin/PrameterizedContentViewer/DefaultStylesheet.css</css-path>
  </portlet-skin>
  
  <!-- ParameterizedContentListViewerPortlet skins -->

  <portlet-skin>
    <application-name>presentation</application-name>
    <portlet-name>ParameterizedContentListViewerPortlet</portlet-name>
    <skin-name>Default</skin-name>
    <css-path>/skin/ContentListViewer/DefaultStylesheet.css</css-path>
  </portlet-skin>
  
  <portlet-skin>
    <application-name>presentation</application-name>
    <portlet-name>ContentListViewerByQueryPortlet</portlet-name>
    <skin-name>Default</skin-name>
    <css-path>/skin/ContentListViewer/DefaultStylesheet.css</css-path>
  </portlet-skin>
  
  <portlet>
    <name>EditingPortlet</name>
    <scripts>
      <depends>
        <scripts>webui</scripts>
      </depends>
    </scripts>
  </portlet>
  
  <portlet>
    <name>UIAdminToolbarPortlet</name>
    <scripts>
      <depends>
        <scripts>navigation</scripts>
      </depends>
    </scripts>
  </portlet>
  
  <portlet>
    <name>SingleContentViewer</name>
    <scripts>
      <depends>
        <scripts>clv</scripts>
      </depends>
      <depends>
        <scripts>wcm-utils</scripts>
      </depends>
      <depends>
        <scripts>base</scripts>
      </depends>
    </scripts>
  </portlet>

  <portlet>
    <name>ContentListViewerPortlet</name>
    <scripts>
      <depends>
        <scripts>clv</scripts>
      </depends>
      <depends>
        <scripts>wcm-utils</scripts>
      </depends>
      <depends>
        <scripts>base</scripts>
      </depends>
    </scripts>
  </portlet>

  <portlet>
    <name>ContentListViewerByQueryPortlet</name>
    <scripts>
      <depends>
        <scripts>clv</scripts>
      </depends>
      <depends>
        <scripts>wcm-utils</scripts>
      </depends>
      <depends>
        <scripts>base</scripts>
      </depends>
    </scripts>
  </portlet>  
  
  
</gatein-resources>


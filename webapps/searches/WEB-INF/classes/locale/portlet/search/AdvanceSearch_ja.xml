<?xml version="1.0" encoding="UTF-8"?>
<bundle>
  <UIDefaultSearchForm>
    <label>
      <keyword>キーワード</keyword>
      <portal>ポータル</portal>
      <Page>ページ</Page>
      <Document>ドキュメント</Document>
    </label>
    <action>
      <Search>検索</Search>
    </action>
  </UIDefaultSearchForm>

  <UIDefaultSearchResult>
    <label>
      <Page>ページ</Page>
      <Document>ドキュメント</Document>
      <DocumentAndPage>ドキュメントとページ</DocumentAndPage>
      <documentandpage>ドキュメントとページ</documentandpage>
      <resutlsCaption>結果</resutlsCaption>
      <ofAboutCaption>of about</ofAboutCaption>
      <forCaption>for</forCaption>
      <timeUnitCaption>秒</timeUnitCaption>
      <similar>同じコンテンツが表示されるページ</similar>
      <editor>編集者</editor>
    </label>
    <action>
      <edit>編集</edit>
    </action>
    <msg>
      <your-search>あなたの検索は</your-search>
      <did-not-match>マッチしませんでした</did-not-match>
      <new-suggestion>提案</new-suggestion>
    </msg>
  </UIDefaultSearchResult>
  <UISearchPageLayoutManager>
    <label>
      <itemsPerPageSelector>ページ毎アイテム数</itemsPerPageSelector>
      <searchFormSelector>検索フォームテンプレート</searchFormSelector>
      <searchResultSelector>検索結果テンプレート</searchResultSelector>
      <searchPaginatorSelector>検索ページングテンプレート</searchPaginatorSelector>
      <searchPageLayoutSelector>検索ページレイアウトテンプレート</searchPageLayoutSelector>
      <searchResultBasePathInputSetAction>ベースパス</searchResultBasePathInputSetAction>
    </label>
    <action>
      <Save>保存</Save>
      <Cancel>キャンセル</Cancel>
    </action>
  </UISearchPageLayoutManager>
  <UISearchForm>
  	 <message>
  	   <keyword-not-support>The keyword is invalid. Some characters are not supported.</keyword-not-support>
  	   <not-checked>Search type is not checked. Choose a search type, please.</not-checked>
  	   <keyword-not-empty>キーワードの値が null または空です！</keyword-not-empty>
  	 </message>
  </UISearchForm>
  <searchOption>
    <label>
      <pageCheckBox>in Pages</pageCheckBox>
      <documentCheckBox>in Content</documentCheckBox>
    </label>
  </searchOption>
  <ViewMode>
    <label>
      <option>
        <all>search all sites</all>
      </option>  
    </label>  
  </ViewMode>  
</bundle>

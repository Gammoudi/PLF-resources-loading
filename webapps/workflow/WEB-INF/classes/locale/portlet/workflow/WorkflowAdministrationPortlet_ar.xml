<?xml version="1.0" encoding="UTF-8"?>
<bundle>
  <!--
  #############################################################################
  #         org.exoplatform.workflow.webui.component.monitor                  #
  #############################################################################
  -->
  <UIWorkflowAdministrationPortlet>
    <tab>
      <label>إدارة</label>
      <text>إدارة سير العمل</text>
    </tab>
		<msg>
      <refresh-session-success>تم تحديث الدورة بنجاح</refresh-session-success>
    </msg>
		<label>
      <refresh>تحديث</refresh>
    </label>
  </UIWorkflowAdministrationPortlet>
  <UITabPane>
    <title>
      <UIMonitor>مراجعة العمليات </UIMonitor>
      <UICompletedProcess>الانتهاء</UICompletedProcess>
      <UIRunningProcess>مستمرة</UIRunningProcess>						
    </title>
  </UITabPane>
  <UIMonitor>
    <header>
      <id>معرف العملية</id>
      <name>اسم العملية</name>
      <version>عملية تنقيح</version>
      <action>عمل</action>
    </header>
    <msg>
      <empty>لا تحكم العمليات التجارية</empty>
    </msg>
  </UIMonitor>
  <UIAdministrationManager>
    <tab>
      <label>
        <UITimers>موقتات</UITimers>
        <UIMonitor>تحكم</UIMonitor>
      </label>
    </tab>
    <msg>
      <confirm-delete-process>هل تريد محو هذا النموذج من العمليات</confirm-delete-process>
    </msg>
    <action>
      <UploadProcess>تحميل عملية</UploadProcess>
      <title>
        <View>تشاور</View>
        <Delete>إزالة</Delete>
      </title>
    </action>
  </UIAdministrationManager>
  <UITimers>
    <header>
      <id>معرف</id>
      <name>اسم</name>
      <dueDate>الموعد النهائي</dueDate>
    </header>
    <msg>
      <empty>لم يقع اخير أي توقيت </empty>
    </msg>
  </UITimers>
  <AdministrationPopup>
    <title>
      <UIProcessDetail>تفاصيل العملية</UIProcessDetail>
    </title>
  </AdministrationPopup>
  <UploadProcessPopup>
    <title>
      <UIUploadProcess>تحميل عملية</UIUploadProcess>
    </title>
  </UploadProcessPopup>
  <UIUploadProcess>
    <label>
      <name>اسم</name>
      <upload>حدد ملف</upload>
    </label>
    <action>
      <Save>#{word.save}</Save>
      <Cancel>#{word.cancel}</Cancel>
    </action>
    <msg>
      <fileName-error>لم يتم العثور على مسار ملف</fileName-error>
      <fileName-invalid>هناك أحرف غير قانونية في اسم الملف. الرجاء إدخال اسم مختلف أو تحميل ملف آخر.</fileName-invalid>
      <process-successful>وكان تحميل ناجح!</process-successful>
    </msg>
  </UIUploadProcess>
  <UIProcessDetailGrid>
    <header>
      <processInstanceId>معرف عملية مثيل</processInstanceId>
      <processId>معرف العملية</processId>
      <processName>اسم العملية</processName>
      <startDate>تاريخ البدء</startDate>
      <endDate>تنتهي</endDate>
      <action>عمل</action>
    </header>
    <msg>
      <empty>أي عملية لمعرفة التفاصيل</empty>
    </msg>
  </UIProcessDetailGrid>
  <UIProcessDetail>
    <msg>
      <confirm-delete-process>هل تريد حذف هذه الحالة عملية تجارية؟</confirm-delete-process>
    </msg>
  </UIProcessDetail>
  <UITasksGrid>
    <header>
      <id>معرف</id>
      <taskName>اسم المهمة</taskName>
      <actorId>هوية الفاعل</actorId>
      <end>نهاية</end>
    </header>
  </UITasksGrid>
  <UIProcessDetail>
    <tab>
      <label>
        <UICompletedProcessGrid>الانتهاء</UICompletedProcessGrid>
        <UIRunningProcessGrid>مستمرة</UIRunningProcessGrid>
      </label>
    </tab>
    <msg>
      <confirm-delete-completed-process>تأكيد أنك تريد حذف عملية الانتهاء؟</confirm-delete-completed-process>
      <result-delete-completed-process>ليست هناك ايت عملية قد انتهت لكي تحذف </result-delete-completed-process>      
    </msg>
    <action>
      <FlushAll>إزالة جميع</FlushAll>
      <Cancel>إلغاء</Cancel>
    </action>    
  </UIProcessDetail>
  <TaskListOfProcessPopup>
    <title>
      <UITaskListOfProcess>عرض قائمة هيئات المهام </UITaskListOfProcess>
    </title>
    <tab>
      <label>
        <UITasksGrid>مهمة</UITasksGrid>
      </label>
    </tab>    
  </TaskListOfProcessPopup>
  <UIRunningProcess>
    <header>
      <processInstanceId>معرف عملية مثيل</processInstanceId>
      <processId>معرف العملية</processId>
      <processName>اسم العملية</processName>
      <startDate>تاريخ البدء</startDate>
      <endDate>تنتهي</endDate>
      <action>عمل</action>
    </header>
    <msg>
      <empty>أي عملية لمعرفة التفاصيل</empty>
    </msg>
    <action>
      <Cancel>إلغاء</Cancel>
    </action>				
  </UIRunningProcess>
  <UICompletedProcess>
    <header>
      <processInstanceId>معرف عملية مثيل</processInstanceId>
      <processId>معرف العملية</processId>
      <processName>اسم العملية</processName>
      <startDate>تاريخ البدء</startDate>
      <endDate>تاريخ الإنتهاء </endDate>
      <action>عمل</action>
    </header>
    <msg>
      <empty>لايوجد عملية لرؤية التفاصيل </empty>
    </msg>
    <action>
      <FlushAll>إزالة جميع</FlushAll>
      <Cancel>إلغاء</Cancel>
    </action>				
  </UICompletedProcess>
  <UITaskListOfProcess>    
    <tab>
      <label>
        <UITasksGrid>قائمة المهام </UITasksGrid>
      </label>
    </tab>
    <action>      
      <Cancel>#{word.cancel}</Cancel>
    </action>    
  </UITaskListOfProcess>
  <!--
  #############################################################################
  #			Since 2.0.0-Alpha02-SNAPSHOT tobe translate											#
  #############################################################################
  -->
	<UIWorkflowAdministrationPortlet>
		<msg>
      <refresh-session-success>تم تحديث الدورة بنجاح </refresh-session-success>
    </msg>
    <label>
      <refresh>تحديث</refresh>
    </label>
  </UIWorkflowAdministrationPortlet>
	
	<UIProcessDetail>
    <action>
    	<title>
    		<View>رأي</View>
				<Delete>حذف</Delete>
    	</title>
    </action>  
  </UIProcessDetail>
	<UIUploadProcess>
    <msg>
      <data-invalid>بعض المشاكل مع البيانات الخاصة بك. لا يمكن العثور على ملف تعريف عملية داخل عملية الأرشفة أو البيانات غير صالحة (العمليات موجودة أو خاطئة شكل...) </data-invalid>
    </msg>
  </UIUploadProcess>
</bundle>

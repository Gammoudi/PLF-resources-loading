<?xml version="1.0" encoding="UTF-8"?>
<bundle>
  <!--
  #############################################################################
  #			org.exoplatform.workflow.webui.component.monitor											#
  #############################################################################
  -->
  <UIWorkflowAdministrationPortlet>
    <tab>
      <label>Administration</label>
      <text>Admin Arbeitsfluss Management</text>
    </tab>
  </UIWorkflowAdministrationPortlet>
  <UITabPane>
    <title>
      <UIMonitor>Prozess Monitor</UIMonitor>
						<UICompletedProcesses>Completed</UICompletedProcesses>
						<UIRunningProcesses>Processing</UIRunningProcesses>
    </title>
  </UITabPane>
  <UIMonitor>
    <header>
      <id>Prozess Id</id>
      <name>Prozessname</name>
      <version>Prozessversion</version>
      <action>Aktion</action>
    </header>
    <msg>
      <empty>Kein Businessprozess zum anzeigen</empty>
    </msg>
  </UIMonitor>
  <UIAdministrationManager>
    <tab>
      <label>
        <UITimers>Timer</UITimers>
        <UIMonitor>Monitor</UIMonitor>
      </label>
    </tab>
    <msg>
      <confirm-delete-process><![CDATA[Wollen Sie dieses Prozessmodell l&ouml;schen?]]></confirm-delete-process>
    </msg>
    <action>
      <UploadProcess>Hochladeprozess</UploadProcess>
      <title>
        <View>Ansehen</View>
        <Delete><![CDATA[L&ouml;schen]]></Delete>
      </title>
    </action>
  </UIAdministrationManager>
  <UITimers>
    <header>
      <id>Id</id>
      <name>Name</name>
      <dueDate>Bis-Datum</dueDate>
    </header>
    <msg>
      <empty>Kein Timer wurde gesetzt</empty>
    </msg>
  </UITimers>
  <AdministrationPopup>
    <title>
      <UIProcessDetail>Prozessdetails</UIProcessDetail>
    </title>
  </AdministrationPopup>
  <UploadProcessPopup>
    <title>
      <UIUploadProcess>Hochladeprozess</UIUploadProcess>
    </title>
  </UploadProcessPopup>
  <UIUploadProcess>
    <label>
      <name>Name</name>
      <upload><![CDATA[Datei ausw&auml;hlen]]></upload>
    </label>
    <action>
      <Save>#{word.save}</Save>
      <Cancel>#{word.cancel}</Cancel>
    </action>
    <msg>
      <fileName-error>Dateipfad nicht gefunden</fileName-error>
      <fileName-invalid><![CDATA[Ung&uuml;ltige Karaktere in Namen der Datei. Tippen Sie den Namen der Datei oder laden Sie eine andere Datei hoch.]]></fileName-invalid>
      <process-successful>Erfolgreich hochgeladen!</process-successful>
    </msg>
  </UIUploadProcess>
  <UICompletedProcesses>
    <header>
      <processInstanceId>Prozess Instanz Id</processInstanceId>
      <processId>Prozess Id</processId>
      <processName>Prozessname</processName>
      <startDate>Anfangsdatum</startDate>
      <endDate>Enddatum</endDate>
      <action>Aktion</action>
    </header>
    <msg>
      <empty>Kein Prozess um Detail anzuschauen</empty>
    </msg>
  </UICompletedProcesses>
		
  <UIRunningProcesses>
    <header>
      <processInstanceId>Prozess Instanz Id</processInstanceId>
      <processId>Prozess Id</processId>
      <processName>Prozessname</processName>
      <startDate>Anfangsdatum</startDate>
      <endDate>Enddatum</endDate>
      <action>Aktion</action>
    </header>
    <msg>
      <empty>Kein Prozess um Detail anzuschauen</empty>
    </msg>
  </UIRunningProcesses>
				
  <UIProcessDetail>
    <msg>
      <confirm-delete-process><![CDATA[Wollen Sie diese Pozess Instanz l&ouml;schen?]]></confirm-delete-process>
    </msg>
  </UIProcessDetail>
  <UITasksGrid>
    <header>
      <id>Id</id>
      <taskName>Aufgabenname</taskName>
      <actorId>Akteur Id</actorId>
      <end>Ende</end>
    </header>
  </UITasksGrid>
  
  <!--
  #############################################################################
  #			Since 2.3.x tobe translate											#
  #############################################################################
  -->
  <UIProcessDetail>
    <msg>
      <confirm-delete-completed-process>Do you want to delete these completed processes?</confirm-delete-completed-process>
      <result-delete-completed-process>There isn't any completed process to delete</result-delete-completed-process>
    </msg>
    <action>
      <FlushAll>Flush All</FlushAll>
      <Cancel>Cancel</Cancel>
    </action>
  </UIProcessDetail>
  <TaskListOfProcessPopup>
    <title>
      <UITaskListOfProcess>List tasks of Instance</UITaskListOfProcess>
    </title>
    <tab>
      <label>
        <UITasksGrid>Task</UITasksGrid>
      </label>
    </tab>
  </TaskListOfProcessPopup>
  <UIRunningProcess>
    <header>
      <processInstanceId>Process Instance Id</processInstanceId>
      <processId>Process Id</processId>
      <processName>Process Name</processName>
      <startDate>Start Date</startDate>
      <endDate>End Date</endDate>
      <action>Action</action>
    </header>
    <msg>
      <empty>No process to view detail</empty>
    </msg>
	    <action>
      <Cancel>Cancel</Cancel>
    </action>			
  </UIRunningProcess>
  <UICompletedProcess>
    <header>
      <processInstanceId>Process Instance Id</processInstanceId>
      <processId>Process Id</processId>
      <processName>Process Name</processName>
      <startDate>Start Date</startDate>
      <endDate>End Date</endDate>
      <action>Action</action>
    </header>
    <msg>
      <empty>No process to view detail</empty>
    </msg>
    <action>
      <FlushAll>Flush All</FlushAll>
      <Cancel>Cancel</Cancel>
    </action>				
  </UICompletedProcess>
  <UITaskListOfProcess>    
    <tab>
      <label>
        <UITasksGrid>List tasks</UITasksGrid>
      </label>
    </tab>
    <action>      
      <Cancel>Cancel</Cancel>
    </action>    
  </UITaskListOfProcess>

  <!--
  #############################################################################
  #			Since 2.0.0-Alpha02-SNAPSHOT tobe translate											#
  #############################################################################
  -->
	
 <UIWorkflowAdministrationPortlet>
		<msg>
      <refresh-session-success>The session has been refreshed successfully</refresh-session-success>
    </msg>
    <label>
      <refresh>Refresh</refresh>
    </label>
  </UIWorkflowAdministrationPortlet>
	<UIProcessDetail>
    <action>
    	<title>
    		<View>View</View>
				<Delete>Delete</Delete>
    	</title>
    </action>  
  </UIProcessDetail>
	<UIUploadProcess>
    <msg>
      <data-invalid>Have some problems with your data. Can't find process definition file inside process archive or data is invalid ( process are existed or wrong format...) </data-invalid>
    </msg>
  </UIUploadProcess>
</bundle>


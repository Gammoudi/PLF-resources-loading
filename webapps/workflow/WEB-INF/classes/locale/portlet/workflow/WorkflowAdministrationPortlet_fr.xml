<?xml version="1.0" encoding="UTF-8"?>
<bundle>
  <!--
  #############################################################################
  #         org.exoplatform.workflow.webui.component.monitor                  #
  #############################################################################
  -->
  <UIWorkflowAdministrationPortlet>
    <tab>
      <label>Administration</label>
      <text>Gestion du Workflow</text>
    </tab>
		<msg>
      <refresh-session-success>La session a été rafraichie avec succès</refresh-session-success>
    </msg>
		<label>
      <refresh>Rafraichir</refresh>
    </label>
  </UIWorkflowAdministrationPortlet>
  <UITabPane>
    <title>
      <UIMonitor>Contrôler le Processus Métier</UIMonitor>
						<UICompletedProcesses>Terminé</UICompletedProcesses>
						<UIRunningProcesses>En cours</UIRunningProcesses>						
    </title>
  </UITabPane>
  <UIMonitor>
    <header>
      <id>Id Processus</id>
      <name>Nom Processus</name>
      <version>Version Processus</version>
      <action>Action</action>
    </header>
    <msg>
      <empty>Aucun processus métier à contrôler</empty>
    </msg>
  </UIMonitor>
  <UIAdministrationManager>
    <tab>
      <label>
        <UITimers>Temporisateurs</UITimers>
        <UIMonitor>Contrôleur</UIMonitor>
      </label>
    </tab>
    <msg>
      <confirm-delete-process>Voulez-vous vraiment supprimer ce modèle de pocessus métier ?</confirm-delete-process>
    </msg>
    <action>
      <UploadProcess>Uploader un processus</UploadProcess>
      <title>
        <View>Consulter</View>
        <Delete>Supprimer</Delete>
      </title>
    </action>
  </UIAdministrationManager>
  <UITimers>
    <header>
      <id>Id</id>
      <name>Nom</name>
      <dueDate>Echéance</dueDate>
    </header>
    <msg>
      <empty>Aucun temporisateur n'a été sélectionné</empty>
    </msg>
  </UITimers>
  <AdministrationPopup>
    <title>
      <UIProcessDetail>Détails du processus</UIProcessDetail>
    </title>
  </AdministrationPopup>
  <UploadProcessPopup>
    <title>
      <UIUploadProcess>Uploader le processus</UIUploadProcess>
    </title>
  </UploadProcessPopup>
  <UIUploadProcess>
    <label>
      <name>Nom</name>
      <upload>Sélectionner un fichier</upload>
    </label>
    <action>
      <Save>#{word.save}</Save>
      <Cancel>#{word.cancel}</Cancel>
    </action>
    <msg>
      <fileName-error>Chemin de fichier introuvable</fileName-error>
      <fileName-invalid>Il existe des caractères interdits dans le nom de fichier. Veuillez saisir un autre nom ou uploader un autre fichier.</fileName-invalid>
      <process-successful>L'upload s'est correctement déroulé !</process-successful>
      <data-invalid>Impossible de trouver la définition du processus dans l'archive ou les données sont invalides. Veuillez vérifier le contenu de l'archive </data-invalid>
    </msg>
  </UIUploadProcess>
  <UIProcessDetailGrid>
    <header>
      <processInstanceId>Id de l'instance du processus</processInstanceId>
      <processId>Id Processus</processId>
      <processName>Nom Processus</processName>
      <startDate>Date de début</startDate>
      <endDate>Date de fin</endDate>
      <action>Action</action>
    </header>
    <msg>
      <empty>Aucun processus pour voir le détail</empty>
    </msg>
  </UIProcessDetailGrid>
  <UIProcessDetail>
    <msg>
      <confirm-delete-process>Voulez-vous vraiment supprimer cet instance de processus métier ?</confirm-delete-process>
    </msg>
  </UIProcessDetail>
  <UITasksGrid>
    <header>
      <id>Id</id>
      <taskName>Nom de Tâche</taskName>
      <actorId>Id Acteur</actorId>
      <end>Fin</end>
    </header>
  </UITasksGrid>
  <UIProcessDetail>
    <msg>
      <confirm-delete-completed-process>Confirmez-vous vouloir supprimer les processus terminés?</confirm-delete-completed-process>
      <result-delete-completed-process>Il n'y a aucun processus terminé à supprimer</result-delete-completed-process>      
    </msg>
    <action>
      <FlushAll>Tout supprimer</FlushAll>
      <Cancel>Annuler</Cancel>
    </action>    
  </UIProcessDetail>
  <TaskListOfProcessPopup>
    <title>
      <UITaskListOfProcess>Lister les tâches de l'instance</UITaskListOfProcess>
    </title>
    <tab>
      <label>
        <UITasksGrid>Tâche</UITasksGrid>
      </label>
    </tab>    
  </TaskListOfProcessPopup>
  <UIRunningProcesses>
    <header>
      <processInstanceId>Id de l'instance du processus</processInstanceId>
      <processId>Id Processus</processId>
      <processName>Nom Processus</processName>
      <startDate>Date de début</startDate>
      <endDate>Date de fin</endDate>
      <action>Action</action>
    </header>
    <msg>
      <empty>Aucun processus pour voir le détail</empty>
    </msg>
    <action>
      <Cancel>Annuler</Cancel>
    </action>				
  </UIRunningProcesses>
  <UICompletedProcesses>
    <header>
      <processInstanceId>Id de l'instance du processus</processInstanceId>
      <processId>Id Processus</processId>
      <processName>Nom Processus</processName>
      <startDate>Date de début</startDate>
      <endDate>Date de fin</endDate>
      <action>Action</action>
    </header>
    <msg>
      <empty>Aucun processus pour voir le détail</empty>
    </msg>
    <action>
      <FlushAll>Tout supprimer</FlushAll>
      <Cancel>Annuler</Cancel>
    </action>				
  </UICompletedProcesses>
  <UITaskListOfProcess>    
    <tab>
      <label>
        <UITasksGrid>Liste des tâches</UITasksGrid>
      </label>
    </tab>
    <action>      
      <Cancel>#{word.cancel}</Cancel>
    </action>    
  </UITaskListOfProcess>
	<UIProcessDetail>
    <action>
    	<title>
    		<View>Visualiser</View>
				<Delete>Supprimer</Delete>
    	</title>
    </action>  
  </UIProcessDetail>
</bundle>

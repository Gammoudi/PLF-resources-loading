<?xml version="1.0" encoding="UTF-8"?>
<bundle>
  <!--
  #############################################################################
  #			org.exoplatform.workflow.webui.component.monitor											#
  #############################################################################
  -->
  <UIWorkflowAdministrationPortlet>
    <tab>
      <label>Administratie</label>
      <text>Workflow Beheer</text>
    </tab>
  </UIWorkflowAdministrationPortlet>
  <UITabPane>
    <title>
      <UIMonitor>Proces Monitor</UIMonitor>
      <UICompletedProcess>Completed</UICompletedProcess>
      <UIRunningProcess>Processing</UIRunningProcess>
    </title>
  </UITabPane>
  <UIMonitor>
    <header>
      <id>Proces Id</id>
      <name>Proces Naam</name>
      <version>Proces Versie</version>
      <action>Actie</action>
    </header>
    <msg>
      <empty>Geen business proces t monitoren</empty>
    </msg>
  </UIMonitor>
  <UIAdministrationManager>
    <tab>
      <label>
        <UITimers>Timers</UITimers>
        <UIMonitor>Monitor</UIMonitor>
      </label>
    </tab>
    <msg>
      <confirm-delete-process>Wilt u deze proces model verwijderen?</confirm-delete-process>
    </msg>
    <action>
      <UploadProcess>Upload proces</UploadProcess>
      <title>
        <View>Consulteer</View>
        <Delete>Verwijder</Delete>
      </title>
    </action>
  </UIAdministrationManager>
  <UITimers>
    <header>
      <id>Id</id>
      <name>Naam</name>
      <dueDate>Datum</dueDate>
    </header>
    <msg>
      <empty>Geen timer gedefinieerd</empty>
    </msg>
  </UITimers>
  <AdministrationPopup>
    <title>
      <UIProcessDetail>Proces Detail</UIProcessDetail>
    </title>
  </AdministrationPopup>
  <UploadProcessPopup>
    <title>
      <UIUploadProcess>Upload proces</UIUploadProcess>
    </title>
  </UploadProcessPopup>
  <UIUploadProcess>
    <label>
      <name>Naam</name>
      <upload>Selecteer een bestand</upload>
    </label>
    <action>
      <Save>#{word.save}</Save>
      <Cancel>#{word.cancel}</Cancel>
    </action>
    <msg>
      <fileName-error>Bestandpad niet gevonden</fileName-error>
      <fileName-invalid>Ongeldige karakters in bestandsnaam. Gelieve bestandsnaam te geven of een andere bestand uploaden.</fileName-invalid>
      <process-successful>Upload successvol!</process-successful>
      <data-invalid>Dataproblemen. Kan processdefinition.xml in proces archief niet terugvinden.</data-invalid>
    </msg>
  </UIUploadProcess>
  <UIProcessDetailGrid>
    <header>
      <processInstanceId>Proces Instantie Id</processInstanceId>
      <processId>Proces Id</processId>
      <processName>Proces Naam</processName>
      <startDate>Start Datum</startDate>
      <endDate>Eind Datum</endDate>
      <action>Actie</action>
    </header>
    <msg>
      <empty>Geen process om details te bezien</empty>
    </msg>
  </UIProcessDetailGrid>
  <UIProcessDetail>
    <msg>
      <confirm-delete-process>Wilt u deze proces instantie verwijderen?</confirm-delete-process>
    </msg>
  </UIProcessDetail>
  <UITasksGrid>
    <header>
      <id>Id</id>
      <taskName>Taak Naam</taskName>
      <actorId>Acteur Id</actorId>
      <end>Eind</end>
    </header>
  </UITasksGrid>
  <!--
  #############################################################################
  #			Since 2.3.x tobe translate											#
  #############################################################################
  -->
  <UIProcessDetail>
    <tab>
      <label>
        <UICompletedProcessGrid>Completed</UICompletedProcessGrid>
        <UIRunningProcessGrid>Processing</UIRunningProcessGrid>
      </label>
    </tab>
    <msg>
      <confirm-delete-completed-process>Do you want to delete these completed processes?</confirm-delete-completed-process>
      <result-delete-completed-process>There isn't any completed process to delete</result-delete-completed-process>
    </msg>
    <action>
      <FlushAll>Flush All</FlushAll>
      <Cancel>Cancel</Cancel>
    </action>    
  </UIProcessDetail>
  <TaskListOfProcessPopup>
    <title>
      <UITaskListOfProcess>List tasks of Instance</UITaskListOfProcess>
    </title>
    <tab>
      <label>
        <UITasksGrid>Task</UITasksGrid>
      </label>
    </tab>    
  </TaskListOfProcessPopup>
  <UIRunningProcess>
    <header>
      <processInstanceId>Process Instance Id</processInstanceId>
      <processId>Process Id</processId>
      <processName>Process Name</processName>
      <startDate>Start Date</startDate>
      <endDate>End Date</endDate>
      <action>Action</action>
    </header>
    <msg>
      <empty>No process to view detail</empty>
    </msg>
    <action>
      <Cancel>Cancel</Cancel>
    </action>				
  </UIRunningProcess>
  <UICompletedProcess>
    <header>
      <processInstanceId>Process Instance Id</processInstanceId>
      <processId>Process Id</processId>
      <processName>Process Name</processName>
      <startDate>Start Date</startDate>
      <endDate>End Date</endDate>
      <action>Action</action>
    </header>
    <msg>
      <empty>No process to view detail</empty>
    </msg>
    <action>
      <FlushAll>Flush All</FlushAll>
      <Cancel>Cancel</Cancel>
    </action>				
  </UICompletedProcess>
  <UITaskListOfProcess>    
    <tab>
      <label>
        <UITasksGrid>List tasks</UITasksGrid>
      </label>
    </tab>
    <action>      
      <Cancel>Cancel</Cancel>
    </action>
  </UITaskListOfProcess>
	
	<!--
  #############################################################################
  #			Since 2.0.0-Alpha02-SNAPSHOT tobe translate											#
  #############################################################################
  -->
	
 <UIWorkflowAdministrationPortlet>
		<msg>
      <refresh-session-success>The session has been refreshed successfully</refresh-session-success>
    </msg>
    <label>
      <refresh>Refresh</refresh>
    </label>
  </UIWorkflowAdministrationPortlet>
	
	<UIProcessDetail>
    <action>
    	<title>
    		<View>View</View>
				<Delete>Delete</Delete>
    	</title>
    </action>  
  </UIProcessDetail>
  <UIUploadProcess>
    <msg>
      <data-invalid>Have some problems with your data. Can't find process definition file inside process archive or data is invalid ( process are existed or wrong format...) </data-invalid>
    </msg>
  </UIUploadProcess>
</bundle>


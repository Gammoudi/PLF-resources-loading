<?xml version="1.0" encoding="UTF-8"?>
<bundle>
  <!--
  #############################################################################
  #			org.exoplatform.workflow.webui.component.controller										#
  #############################################################################
  -->
  <UIWorkflowControllerPortlet>
    <tab>
      <label>Controller</label>
      <text>Admin Workflow Management</text>
    </tab>
    <msg>
      <refresh-session-success>The session was refreshed successfully.</refresh-session-success>
    </msg>
    <label>
      <refresh>Refresh</refresh>
    </label>
  </UIWorkflowControllerPortlet>
  <ControllerPopup>
    <title>
      <UITaskManager>Task Management</UITaskManager>
    </title>
  </ControllerPopup>
  <UITabPane>
    <title>
      <UITaskList>Task Controller</UITaskList>
      <UIBPDefinition>BP Definition Controller</UIBPDefinition>
      <UIDocumentContent>View Content</UIDocumentContent>
      <UIDocumentForm>Edit Content</UIDocumentForm>
      <UITask>Task</UITask>
    </title>
  </UITabPane>
  <UITaskList>
    <header>
      <id>Task Id</id>
      <processName>Process Name</processName>
      <taskName>Task Name</taskName>
      <actor>Actor</actor>
      <description>Description</description>
      <actorId>Actor Id</actorId>
      <taskVariables>Task Variables</taskVariables>
      <processDateStart>Process Date Start</processDateStart>
      <action>Action</action>
    </header>
    <label>
      <managerState>Manage</managerState>
    </label>
    <msg>
      <task-not-found>You cannot manage the task. Maybe, it has been already managed by another user.</task-not-found>
      <task-change>The task was changed or removed.</task-change>
    </msg>
  </UITaskList>
  <UITaksList>
    <msg>
      <no-task>No assigned tasks</no-task>
    </msg>
  </UITaksList>
  <UITask>
    <label>
      <hide-process-link>Hide Process Diagram</hide-process-link>
      <show-process-link>Show Process Diagram</show-process-link>
      <process-diagram-not-available>Process's diagram is not available</process-diagram-not-available>
    </label>
    <title>
      <Delegate>Select User</Delegate>
      <state-image>State Image</state-image>
    </title>
    <msg>
      <has-not-got-delegate>You must select any users to delegate.</has-not-got-delegate>
      <user-not-exist>This delegated user does not exist.</user-not-exist>
    </msg>
  </UITask>
  <PopupDelegatedSelectorId>
    <title>
      <UIUserSelectContainer>Select User</UIUserSelectContainer>
    </title>
  </PopupDelegatedSelectorId>
  <UIBPDefinition>
    <header>
      <id>Id</id>
      <name>Name</name>
      <version>Version</version>
      <action>Action</action>
    </header>
    <msg>
      <empty>No business process to start</empty>
    </msg>
  </UIBPDefinition>
  <UIControllerManager>
    <action>
      <title>
        <ManageStart>Manage Start</ManageStart>
      </title>
    </action>
  </UIControllerManager>
  <UIDocumentForm>
    <msg>
      <cannot-save>An error occurred while saving the document.</cannot-save>
      <in-versioning>Please check out the current node before adding a new one.</in-versioning>
      <not-support>Cannot load the template of '{0}'.</not-support>
      <value-invalid>'{0}' is required.</value-invalid>
    </msg>
  </UIDocumentForm>
  <PopupComponent>
    <title>
      <UIOneTaxonomySelector>Add Categories</UIOneTaxonomySelector>
    </title>
  </PopupComponent>
  <UITreeTaxonomyList>
    <label>
      <taxonomyTree>Category Tree</taxonomyTree>
      <rootNodePath>Root Tree</rootNodePath>
      <rootNodeInfo>UITreeTaxonomyList</rootNodeInfo>
    </label>
  </UITreeTaxonomyList>
     <UIWorkspaceList>
    <label>
      <workspaceName>Workspace</workspaceName>
      <rootNodeInfo>UIWorkspaceList</rootNodeInfo>
      <rootNodePath>Root Path</rootNodePath>
    </label>
  </UIWorkspaceList>
  <rootNodeInfo>
    <title>
      <AddRootNode>Add Root Node</AddRootNode>
    </title>
  </rootNodeInfo>
  <PopupComponent>
    <title>
      <UIOneNodePathSelector>Select Path</UIOneNodePathSelector>
    </title>
  </PopupComponent>
  <UIDocumentForm>
   <msg>
     <confirm-close>The changes you made will be lost if you close this form.</confirm-close>
     <confirm-navigate>The changes you made will be lost if you navigate away from this page.</confirm-navigate>
   </msg>
  </UIDocumentForm>
</bundle>